<script>
  (function () {
    const pageSize = 12;
    const page = {{ .Page }};
    const totalPages = {{ .TotalPages }};
    const filtered = {{ .FilteredCount }};
    const prevURL = "{{ .PrevURL }}";
    const nextURL = "{{ .NextURL }}";

    const summary = document.querySelector("[data-pagination-summary]");
    if (summary) {
      const start = filtered === 0 ? 0 : (page-1) * pageSize + 1;
      const end = Math.min(filtered, page * pageSize);
      const plural = filtered > 1 ? "s" : "";
      summary.textContent = `Montrant ${start} à ${end} de ${filtered} résultat${plural}`;
    }

    const disableNode = (node) => {
      if (!node) return;
      node.setAttribute("aria-disabled", "true");
      node.classList.add("opacity-50", "pointer-events-none");
    };

    const prevButtons = document.querySelectorAll("[data-pagination-prev]");
    prevButtons.forEach((btn) => {
      if (!prevURL) {
        disableNode(btn);
        return;
      }
      btn.addEventListener("click", function (event) {
        event.preventDefault();
        window.location.href = prevURL;
      });
    });

    const nextButtons = document.querySelectorAll("[data-pagination-next]");
    nextButtons.forEach((btn) => {
      if (!nextURL) {
        disableNode(btn);
        return;
      }
      btn.addEventListener("click", function (event) {
        event.preventDefault();
        window.location.href = nextURL;
      });
    });
  })();

  (function () {
    const modalName = "recipe-filters";
    const modal = document.querySelector(`dialog[data-modal="${modalName}"]`);
    if (!modal || typeof modal.showModal !== "function") return;

    const openers = document.querySelectorAll("[data-flux-modal-trigger]");
    openers.forEach((trigger) => {
      trigger.addEventListener("click", function (event) {
        const disabledButton = trigger.querySelector("button[disabled]");
        if (disabledButton) return;
        event.preventDefault();
        modal.showModal();
      });
    });

    const closers = modal.querySelectorAll("[data-flux-modal-close] button");
    closers.forEach((btn) => {
      btn.addEventListener("click", function (event) {
        event.preventDefault();
        modal.close();
      });
    });
  })();

  (function () {
    const normalize = (s) => (s || "").trim().toLowerCase();
    const setPageAndNavigate = (updates) => {
      const url = new URL(window.location.href);
      Object.entries(updates).forEach(([key, value]) => {
        if (value === null || value === undefined || value === "") {
          url.searchParams.delete(key);
        } else {
          url.searchParams.set(key, value);
        }
      });
      url.searchParams.set("page", "1");
      window.location.href = url.toString();
    };

    const sortMap = {
      "hellofresh_created_at-desc": "title-desc",
      "hellofresh_created_at-asc": "title-asc",
      "hellofresh_updated_at-desc": "title-desc",
      "hellofresh_updated_at-asc": "title-asc",
      "prep_time-asc": "prep-asc",
      "prep_time-desc": "prep-desc",
      "difficulty-asc": "difficulty-asc",
      "difficulty-desc": "difficulty-desc",
      "title-asc": "title-asc",
      "title-desc": "title-desc",
    };
    const inverseSortMap = {
      "title-asc": "hellofresh_created_at-asc",
      "title-desc": "hellofresh_created_at-desc",
      "prep-asc": "prep_time-asc",
      "prep-desc": "prep_time-desc",
      "difficulty-asc": "difficulty-asc",
      "difficulty-desc": "difficulty-desc",
    };
    const params = new URL(window.location.href).searchParams;

    document.addEventListener("click", (event) => {
      const option = event.target.closest("ui-option");
      if (!option) return;
      const select = option.closest('[wire\\:model\\.live="sortBy"]');
      if (!select) return;

      const rawValue = option.getAttribute("value");
      if (!rawValue) return;
      const mapped = sortMap[rawValue] || sortMap["title-asc"];
      setPageAndNavigate({ sort: mapped });
    });

    const difficultyBoxes = Array.from(
      document.querySelectorAll('ui-checkbox[value="1"], ui-checkbox[value="2"], ui-checkbox[value="3"]')
    );
    const difficultyMap = { "1": "facile", "2": "moyen", "3": "difficile" };

    const syncDifficulty = () => {
      const selected = difficultyBoxes.filter((box) => box.hasAttribute("data-checked"));
      if (selected.length === 1) {
        const value = selected[0].getAttribute("value");
        setPageAndNavigate({ difficulty: difficultyMap[value] || "" });
      } else {
        setPageAndNavigate({ difficulty: "" });
      }
    };

    difficultyBoxes.forEach((box) => {
      box.addEventListener("click", () => setTimeout(syncDifficulty, 50));
      box.addEventListener("keyup", (event) => {
        if (event.key === "Enter" || event.key === " ") {
          setTimeout(syncDifficulty, 50);
        }
      });
    });

    const tagBox = document.querySelector('[wire\\:model\\.live="tagIds"]');
    const syncTag = () => {
      if (!tagBox) return;
      const selected = tagBox.querySelector("ui-option[data-selected]");
      if (!selected) {
        setPageAndNavigate({ tag: "" });
        return;
      }
      const text = normalize(selected.textContent);
      setPageAndNavigate({ tag: text });
    };
    document.addEventListener("click", (event) => {
      const option = event.target.closest("ui-option");
      if (!option) return;
      if (option.closest('[wire\\:model\\.live="tagIds"]')) {
        setTimeout(syncTag, 50);
      }
    });
    document.addEventListener("keyup", (event) => {
      const option = event.target.closest("ui-option");
      if (!option) return;
      if (option.closest('[wire\\:model\\.live="tagIds"]') && (event.key === "Enter" || event.key === " ")) {
        setTimeout(syncTag, 50);
      }
    });

    const applyInitialSort = () => {
      const current = params.get("sort");
      if (!current) return;
      const optionValue = inverseSortMap[current];
      if (!optionValue) return;
      const currentOption = document.querySelector(`[wire\\:model\\.live="sortBy"] ui-option[value="${optionValue}"]`);
      if (currentOption && !currentOption.hasAttribute("data-selected")) {
        currentOption.setAttribute("data-selected", "");
        currentOption.setAttribute("aria-selected", "true");
      }
    };

    const applyInitialDifficulty = () => {
      const value = params.get("difficulty");
      if (!value) return;
      const targetEntry = Object.entries(difficultyMap).find(([, label]) => normalize(label) === normalize(value));
      if (!targetEntry) return;
      const [key] = targetEntry;
      difficultyBoxes.forEach((box) => {
        if (box.getAttribute("value") === key) {
          box.setAttribute("data-checked", "");
          box.setAttribute("aria-checked", "true");
        } else {
          box.removeAttribute("data-checked");
          box.setAttribute("aria-checked", "false");
        }
      });
    };

    const applyInitialTag = () => {
      const value = normalize(params.get("tag"));
      if (!value || !tagBox) return;
      const options = Array.from(tagBox.querySelectorAll("ui-option"));
      const target = options.find((opt) => normalize(opt.textContent) === value);
      if (!target) return;
      target.setAttribute("data-selected", "");
      target.setAttribute("aria-selected", "true");
    };

    applyInitialSort();
    applyInitialDifficulty();
    applyInitialTag();
  })();
</script>
