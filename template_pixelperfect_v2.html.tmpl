<script>
(() => {
  const normalize = (s) => (s || "").replace(/\s+/g, " ").trim();

  const multiplyNumbersInText = (text, factor) => {
    const src = normalize(text);
    if (!src) return src;

    const re = /(\d+(?:[.,]\d+)?)/g;
    if (!re.test(src)) return src;
    re.lastIndex = 0;

    return src.replace(re, (m) => {
      const n = parseFloat(m.replace(",", "."));
      if (!Number.isFinite(n)) return m;
      const v = Math.round(n * factor * 100) / 100;
      return String(v);
      // FR (virgule) :
      // return String(v).replace(".", ",");
    });
  };

  function getIngredientsCard() {
    const cards = Array.from(document.querySelectorAll('[data-flux-card]'));
    return cards.find(card => {
      const h = card.querySelector('[data-flux-heading]');
      return h && normalize(h.textContent) === "Ingrédients";
    }) || null;
  }

  function getYieldButtons(card) {
    return Array.from(card.querySelectorAll('button[wire\\:click*="selectedYield"]'));
  }

  function collectQtyNodes(card) {
    const rows = Array.from(card.querySelectorAll('div.flex.items-center.gap-3'));
    const qtyNodes = rows.map(row => {
      const ps = row.querySelectorAll('p');
      return ps.length ? ps[ps.length - 1] : null;
    }).filter(Boolean);

    for (const p of qtyNodes) {
      if (!p.dataset.baseQty) p.dataset.baseQty = normalize(p.textContent);
    }
    return qtyNodes;
  }

  // On veut EXACTEMENT le vert du bouton "1" au départ
  let ACTIVE_CLASS = null;   // classe du bouton 1 au chargement (vert)
  let INACTIVE_CLASS = null; // classe des autres boutons au chargement

  function bootstrapButtonClasses(buttons) {
    if (!buttons.length) return;

    // N'initialiser qu'une fois (pour garder "le vert du bouton 1 au debut")
    if (!ACTIVE_CLASS) {
      const b1 = buttons.find(b => parseInt(normalize(b.textContent), 10) === 1) || buttons[0];
      ACTIVE_CLASS = b1.className;

      const other = buttons.find(b => parseInt(normalize(b.textContent), 10) !== 1);
      INACTIVE_CLASS = other ? other.className : b1.className; // fallback si un seul bouton
    }
  }

  function setActiveButton(card, yieldValue) {
    const buttons = getYieldButtons(card);
    if (!buttons.length) return;

    bootstrapButtonClasses(buttons);

    // Met tous les boutons en "inactif", puis celui cliqué en "actif"
    for (const b of buttons) {
      b.className = INACTIVE_CLASS;
      b.setAttribute("aria-pressed", "false");
    }

    const active = buttons.find(b => parseInt(normalize(b.textContent), 10) === yieldValue);
    if (active) {
      active.className = ACTIVE_CLASS;
      active.setAttribute("aria-pressed", "true");
    }
  }

  function applyYield(card, yieldValue) {
    const factor = Number(yieldValue);
    if (!Number.isFinite(factor) || factor <= 0) return;

    const qtyNodes = collectQtyNodes(card);
    for (const p of qtyNodes) {
      const base = p.dataset.baseQty || normalize(p.textContent);
      p.textContent = multiplyNumbersInText(base, factor);
    }
  }

  function init() {
    const card = getIngredientsCard();
    if (!card) return;

    // init (1 portion)
    const buttons = getYieldButtons(card);
    if (!buttons.length) return;
    bootstrapButtonClasses(buttons);

    setActiveButton(card, 1);
    applyYield(card, 1);

    // Click-only (fluide) + re-apply 1 fois après (si rerender)
    card.addEventListener("click", (e) => {
      const btn = e.target.closest('button[wire\\:click*="selectedYield"]');
      if (!btn) return;

      e.preventDefault();
      e.stopPropagation();
      if (e.stopImmediatePropagation) e.stopImmediatePropagation();

      const n = parseInt(normalize(btn.textContent), 10);
      if (!Number.isFinite(n)) return;

      // Applique sur le DOM actuel
      const cardNow = getIngredientsCard();
      if (!cardNow) return;
      setActiveButton(cardNow, n);
      applyYield(cardNow, n);

      // Si Livewire/Alpine rerender juste après le clic, on ré-applique une fois
      setTimeout(() => {
        const card2 = getIngredientsCard();
        if (!card2) return;
        setActiveButton(card2, n);
        applyYield(card2, n);
      }, 30);
    }, true);
  }

  if (document.readyState === "loading") {
    document.addEventListener("DOMContentLoaded", init);
  } else {
    init();
  }
})();
</script>

